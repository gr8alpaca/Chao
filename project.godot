; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Chao"
run/main_scene="res://scenes/main.tscn"
config/project_settings_override="override.cfg"
config/features=PackedStringArray("4.4", "GL Compatibility")
boot_splash/bg_color=Color(0, 0, 0, 1)
boot_splash/show_image=false

[autoload]

Event="*res://autoload/Event.gd"

[debug]

gdscript/warnings/unassigned_variable=0
gdscript/warnings/unused_variable=0
gdscript/warnings/unused_local_constant=0
gdscript/warnings/unused_private_class_variable=0
gdscript/warnings/unused_parameter=0
gdscript/warnings/unused_signal=0
gdscript/warnings/shadowed_variable=0
gdscript/warnings/shadowed_variable_base_class=0
gdscript/warnings/unreachable_code=0
gdscript/warnings/standalone_expression=0
gdscript/warnings/standalone_ternary=0
gdscript/warnings/incompatible_ternary=0
gdscript/warnings/unsafe_void_return=0
gdscript/warnings/assert_always_false=0
gdscript/warnings/integer_division=0
gdscript/warnings/narrowing_conversion=0
gdscript/warnings/int_as_enum_without_cast=0
gdscript/warnings/empty_file=0

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/window_width_override=1920
window/size/window_height_override=1080
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"

[editor]

naming/default_signal_callback_name="_{signal_name}"
naming/default_signal_callback_to_self_name="_{signal_name}"
naming/script_name_casing=2

[file_customization]

folder_colors={
"res://assets/": "pink",
"res://autoload/": "teal",
"res://models/": "orange",
"res://resources/": "gray",
"res://scenes/": "blue",
"res://script_templates/": "gray",
"res://scripts/": "yellow"
}

[gui]

theme/default_theme_scale=2.0
theme/custom="res://theme.tres"
theme/default_font_multichannel_signed_distance_field=true

[input]

back={
"deadzone": 0.5,
"events": [Object(InputEventKey,"resource_local_to_scene":false,"resource_name":"","device":-1,"window_id":0,"alt_pressed":false,"shift_pressed":false,"ctrl_pressed":false,"meta_pressed":false,"pressed":false,"keycode":0,"physical_keycode":4194308,"key_label":0,"unicode":0,"location":0,"echo":false,"script":null)
]
}

[layer_names]

3d_physics/layer_1="Physical"
3d_physics/layer_2="Input"
3d_physics/layer_3="Pet"

[physics]

3d/default_gravity=1.0

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
environment/defaults/default_environment="res://resources/environment_default.tres"

[shader_globals]

TIME_SCALE={
"type": "float",
"value": 1.0
}
