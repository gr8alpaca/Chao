; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="LudemDare56"
config/features=PackedStringArray("4.3", "GL Compatibility")
boot_splash/show_image=false

[debug]

gdscript/warnings/unassigned_variable=0
gdscript/warnings/unused_variable=0
gdscript/warnings/unused_local_constant=0
gdscript/warnings/unused_private_class_variable=0
gdscript/warnings/unused_parameter=0
gdscript/warnings/unused_signal=0
gdscript/warnings/shadowed_variable=0
gdscript/warnings/unreachable_code=0
gdscript/warnings/standalone_expression=0
gdscript/warnings/standalone_ternary=0
gdscript/warnings/unsafe_void_return=0
gdscript/warnings/assert_always_false=0
gdscript/warnings/integer_division=0
gdscript/warnings/narrowing_conversion=0
gdscript/warnings/int_as_enum_without_cast=0
gdscript/warnings/empty_file=0

[display]

window/size/viewport_width=480
window/size/viewport_height=360
window/size/always_on_top=true
window/size/window_width_override=1920
window/size/window_height_override=1440
window/stretch/aspect=4

[editor]

naming/default_signal_callback_name="_{signal_name}"
naming/default_signal_callback_to_self_name="_{signal_name}"
naming/script_name_casing=2

[file_customization]

folder_colors={
"res://assets/": "pink",
"res://autoload/": "teal",
"res://resources/": "gray",
"res://scenes/": "blue",
"res://scripts/": "yellow"
}

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
